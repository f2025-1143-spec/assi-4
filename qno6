#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    int N;
    cin >> N;
 int mat[100][100];
    int arr[10000]; 
    int k = 0;
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            cin >> mat[i][j];
            arr[k++] = mat[i][j];
        }
    }
    sort(arr, arr + k);
    k = 0;
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            mat[i][j] = arr[k++];
        }
    }
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            cout << mat[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}
